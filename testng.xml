<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
  
<suite name="Suite" parallel="tests" verbose="1" >
    <listeners>
        <listener class-name="com.example.Listeners"/>
    </listeners>

    <test name="validating user login" >
        <classes>
        <class name="com.example.HomePage" />
        </classes>
    </test>
    <test name="validating navbar" >
        <classes>
        <class name="com.example.ValidateNavBar" />
        </classes>
    </test>
    <test name="validating title" >
        <classes>
        <class name="com.example.ValidateTitle" />
        </classes>
    </test>
</suite>
